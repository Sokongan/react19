model User {
  id          String         @id @default(cuid())
  username    String      @unique
  email       String      @unique
  password    String
  first_name  String
  middle_name String?
  last_name   String
  active      Boolean
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
  session     Session[]
  accessLog   AccessLog[]
  roles       Role[]      @relation("UserRoles")
}
